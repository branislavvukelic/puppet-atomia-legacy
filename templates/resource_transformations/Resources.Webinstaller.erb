<resourceDescription xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <bindings xdt:Locator="XPath(//bindings[moduleList/module/@name='Atomia.Provisioning.Modules.WebInstaller.WebInstaller'])">
    <resourceList>
      <resource name="Webinstaller" xdt:Transform="Insert">
        <property name="URL">http://<%= IO.popen("#{@lookup_var} webinstaller webinstaller_ip #{@hierapath} #{@modulepath}").read.gsub(/\n/,'').gsub(/\r/,'').gsub(/\n/,"").gsub(/\r/,"") %>/webinstaller</property>
        <property name="Username"><%= IO.popen("#{@lookup_var} webinstaller agent_username #{@hierapath} #{@modulepath}").read.gsub(/\n/,'').gsub(/\r/,'') %></property>
        <property name="Password"><%= IO.popen("#{@lookup_var} webinstaller agent_password #{@hierapath} #{@modulepath}").read.gsub(/\n/,'').gsub(/\r/,'') %></property>
        <property name="Timeout">600</property>
	<property name="StoragePath"><%= IO.popen("#{@lookup_var} webinstaller content_mount_point #{@hierapath} #{@modulepath}").read.gsub(/\n/,'').gsub(/\r/,'') %></property>
      </resource>
    </resourceList>
  </bindings>
</resourceDescription>
