<resourceDescription xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <bindings xdt:Locator="XPath(//bindings[moduleList/module/@name='Atomia.Provisioning.Modules.AWStats.AWStats'])">
    <resourceList>
      <resource name="AWStats" xdt:Transform="Insert">
        <property name="URL">http://<%= IO.popen("#{@lookup_var} awstats awstats_ip #{@hierapath} #{@modulepath}").read.gsub(/\n/,'').gsub(/\r/,'').gsub(/\n/,"").gsub(/\r/,"") %>:8888/AwstatsAgentService</property>
        <property name="Username"><%= IO.popen("#{@lookup_var} awstats agent_user #{@hierapath} #{@modulepath}").read.gsub(/\n/,'').gsub(/\r/,'') %></property>
        <property name="Password"><%= IO.popen("#{@lookup_var} awstats agent_password #{@hierapath} #{@modulepath}").read.gsub(/\n/,'').gsub(/\r/,'') %></property>
        <property name="ReportsSiteIP"><%= IO.popen("#{@lookup_var} awstats awstats_ip #{@hierapath} #{@modulepath}").read.gsub(/\n/,'').gsub(/\r/,'').gsub(/\n/,"") %></property>
      </resource>
    </resourceList>
  </bindings>
</resourceDescription>
